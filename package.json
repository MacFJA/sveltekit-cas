{
	"name": "@macfja/sveltekit-cas",
	"description": "CAS/SSO integration in SvelteKit",
	"module": "dist/index.mjs",
	"main": "dist/index.js",
	"version": "1.0.0",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist/",
		"types/",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"doc": "typedoc src/index.ts --out docs && cp LICENSE.md CONTRIBUTING.md docs/.",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "eslint src/",
		"prebuild": "tsc",
		"build": "rollup -c",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-typescript": "^8.3.0",
		"@sveltejs/kit": "^1.0.0-next.201",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"eslint": "^8.4.0",
		"express": "^4.17.1",
		"node-fetch": "^3.1.0",
		"prettier": "^2.5.1",
		"rollup": "^2.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.5.0",
		"tslib": "^2.3.1",
		"typedoc": "^0.22.10",
		"typescript": "^4.5.2"
	},
	"dependencies": {
		"env-var": "^7.1.1",
		"logical-cas-client": "^0.0.4",
		"njwt": "^1.2.0",
		"universal-cookie": "^4.0.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/macfja/sveltekit-cas.git"
	},
	"bugs": {
		"url": "https://github.com/macfja/sveltekit-cas/issues"
	},
	"homepage": "https://github.com/macfja/sveltekit-cas#readme",
	"author": "MacFJA",
	"license": "MIT",
	"types": "types/index.d.ts",
	"keywords": [
		"sso",
		"cas",
		"svelte",
		"sveltejs",
		"sveltekit"
	]
}
